{
  "name": "ang-api",
  "version": "1.16.7",
  "description": "Nuxt.js project",
  "author": "Yannick Schurter <yannick.schurter@gmail.com>",
  "private": true,
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "dev": "nodemon -w server.js -w app.js -w nuxt.config.js -w api/ -w lib/ -w store/ server.js",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server.js",
    "precommit": "npm run lint",
    "test": "mocha --exit",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "patch-version": "sed -i \"s#\\(image: ezpaarseproject/analogist:\\)\\([\\.a-z0-9]\\+\\)#\\1${npm_package_version}#g\" docker-compose.yml",
    "version": "npm run patch-version && git add docker-compose.yml"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "config": "^1.30.0",
    "connect-mongo": "^2.0.1",
    "cors": "^2.8.4",
    "cross-env": "^5.2.0",
    "deep-equal": "^1.0.1",
    "es6-promise": "^4.2.4",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "file-saver": "^1.3.8",
    "grant-express": "^3.8.2",
    "mdi": "^2.2.43",
    "moment": "^2.22.2",
    "mongodb": "^3.1.1",
    "morgan": "^1.9.1",
    "nodemailer": "^4.6.7",
    "nuxt": "^1.4.1",
    "request": "^2.87.0",
    "socket.io": "^2.1.1",
    "socket.io-client": "^2.1.1",
    "vue-i18n": "^8.0.0",
    "vuedraggable": "^2.16.0",
    "vuetify": "^1.1.8"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^5.2.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-html": "^4.0.5",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "husky": "^0.14.3",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.3",
    "supertest": "^3.1.0"
  }
}
