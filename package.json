{
  "name": "ang-api",
  "version": "1.16.7",
  "description": "Nuxt.js project",
  "author": "Yannick Schurter <yannick.schurter@gmail.com>",
  "private": true,
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon -w server.js -w app.js -w nuxt.config.js -w api/ -w lib/ -w store/ server.js",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server.js",
    "precommit": "npm run lint",
    "test": "mocha --exit",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "patch-version": "sed -i \"s#\\(image: ezpaarseproject/analogist:\\)\\([\\.a-z0-9]\\+\\)#\\1${npm_package_version}#g\" docker-compose.yml",
    "version": "npm run patch-version && git add docker-compose.yml",
    "generate": "nuxt generate"
  },
  "dependencies": {
    "@mdi/font": "^4.7.95",
    "@nuxtjs/auth": "^4.8.5",
    "@nuxtjs/axios": "^5.9.3",
    "axios": "^0.18.1",
    "body-parser": "^1.19.0",
    "config": "^1.31.0",
    "connect-mongo": "^2.0.3",
    "cors": "^2.8.5",
    "cross-env": "^5.2.1",
    "deep-equal": "^1.1.1",
    "es6-promise": "^4.2.8",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "file-saver": "^1.3.8",
    "grant-express": "^3.8.2",
    "mdi": "^2.2.43",
    "mjml": "^4.5.1",
    "moment": "^2.24.0",
    "mongodb": "^3.4.1",
    "morgan": "^1.9.1",
    "nodemailer": "^4.7.0",
    "nunjucks": "^3.2.0",
    "nuxt": "^2.11.0",
    "request": "^2.88.0",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "vue-i18n": "^8.15.3",
    "vuedraggable": "^2.23.2"
  },
  "devDependencies": {
    "@nuxtjs/vuetify": "^1.9.1",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-html": "^4.0.6",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "husky": "^0.14.3",
    "mocha": "^5.2.0",
    "nodemon": "^1.19.4",
    "supertest": "^3.4.2"
  }
