{
  "name": "ang-api",
  "version": "1.8.2",
  "description": "Nuxt.js project",
  "author": "Yannick Schurter <yannick.schurter@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "nodemon -w server.js -w app.js -w nuxt.config.js -w api/ -w lib/ server.js",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server.js",
    "precommit": "npm run lint",
    "test": "mocha",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "patch-version": "sed -i \"s#\\(image: ezpaarseproject/analogist:\\)\\([\\.a-z0-9]\\+\\)#\\1${npm_package_version}#g\" docker-compose.yml",
    "version": "npm run patch-version && git add docker-compose.yml"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "body-parser": "^1.18.1",
    "config": "^1.26.1",
    "connect-mongo": "^1.3.2",
    "cross-env": "^5.0.4",
    "deep-equal": "^1.0.1",
    "es6-promise": "^4.1.1",
    "express": "^4.15.4",
    "express-session": "^1.15.5",
    "file-saver": "^1.3.3",
    "grant-express": "^3.8.0",
    "mdi": "^2.0.46",
    "moment": "^2.18.1",
    "mongodb": "^2.2.30",
    "morgan": "^1.8.2",
    "nodemailer": "^4.1.0",
    "nuxt": "^1.0.0-rc11",
    "request": "^2.81.0",
    "vue-i18n": "^7.2.0",
    "vuedraggable": "^2.14.1",
    "vuetify": "^0.15.7"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^4.6.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-html": "^3.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "~3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "husky": "^0.14.3",
    "mocha": "^3.5.3",
    "nodemon": "^1.12.0",
    "supertest": "~3.0.0"
  }
}
