{
  "name": "ang-api",
  "version": "1.17.20",
  "description": "Nuxt.js project",
  "author": "Yannick Schurter <yannick.schurter@gmail.com>",
  "private": true,
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon -w server.js -w app.js -w nuxt.config.js -w api/ -w lib/ -w store/ server.js",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server.js",
    "precommit": "npm run lint",
    "test": "mocha --exit",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "patch-version": "sed -i \"s#\\(image: ezpaarseproject/analogist:\\)\\([\\.a-z0-9]\\+\\)#\\1${npm_package_version}#g\" docker-compose.yml",
    "version": "npm run patch-version && git add docker-compose.yml",
    "generate": "nuxt generate"
  },
  "dependencies": {
    "@nuxtjs/auth": "^4.9.1",
    "@nuxtjs/axios": "^5.11.0",
    "body-parser": "^1.19.0",
    "config": "^3.3.1",
    "connect-mongo": "^3.2.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "date-fns": "^2.16.1",
    "deep-equal": "^2.0.3",
    "es6-promise": "^4.2.8",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "file-saver": "^2.0.2",
    "grant-express": "^3.8.2",
    "mdi": "^2.2.43",
    "mjml": "^4.6.3",
    "mongodb": "^3.5.9",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.10",
    "nunjucks": "^3.2.1",
    "nuxt": "^2.13.3",
    "nuxt-i18n": "^6.15.4",
    "request": "^2.88.2",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "vuedraggable": "^2.24.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@mdi/font": "^5.8.55",
    "@nuxtjs/vuetify": "^1.11.2",
    "chai": "^4.2.0",
    "eslint": "^7.4.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-html": "^6.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "^0.14.3",
    "mocha": "^8.0.1",
